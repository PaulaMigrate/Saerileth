//AUTHOR ~gilalion@gmail.com~

SUPPORT ~PaulaMigrate@mail.com~

VERSION ~v20~


ALWAYS
ACTION_IF GAME_IS ~eet~ BEGIN
    OUTER_SET bg2_chapter = 12
  END ELSE BEGIN
    OUTER_SET bg2_chapter = 0
  END
 OUTER_FOR (i=1; i<=10; i=i+1) BEGIN
    OUTER_SET bg2_chapter = bg2_chapter + 1
    OUTER_SPRINT name_source ~bg2_chapter_%i%~
    OUTER_SET EVAL ~%name_source%~ = bg2_chapter
  END

  ACTION_DEFINE_ARRAY nsnoconvert BEGIN END
  ACTION_DEFINE_ARRAY nsreload BEGIN setup END

  LAF HANDLE_CHARSETS
    INT_VAR
      infer_charset = 1
    STR_VAR
      tra_path = EVAL ~%MOD_FOLDER%~
      noconvert_array = nsnoconvert
      reload_array = nsreload
  END
END

